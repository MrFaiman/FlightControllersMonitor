{
  "compilerOptions": {
    "baseUrl": ".",
		"paths": {
			"@utils/*": ["src/utils/*"],
			"@routes/*": ["src/routes/*"],
			"@models/*": ["src/models/*"]
		},
		"target": "ES6",
		"module": "CommonJS",
		"outDir": "./dist",
		"rootDir": "./src",
		"strict": true,
		"esModuleInterop": true
	},
	"include": ["src/**/*.ts"]
}
